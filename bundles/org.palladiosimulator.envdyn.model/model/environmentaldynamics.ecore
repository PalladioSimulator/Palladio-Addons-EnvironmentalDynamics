<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="environment" nsURI="http://palladiosimulator.org/Environment/1.0"
    nsPrefix="environment">
  <eSubpackages name="template" nsURI="http://palladiosimulator.org/Environment/Template/1.0"
      nsPrefix="template">
    <eClassifiers xsi:type="ecore:EClass" name="TemplateDefinitions" eSuperTypes="platform:/plugin/tools.mdsd.modelingfoundations.identifier/model/identifier.ecore#//Entity">
      <eStructuralFeatures xsi:type="ecore:EReference" name="variables" upperBound="-1"
          eType="#//template/TemplateVariable" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="factors" upperBound="-1"
          eType="#//template/TemplateFactor" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="arguments" upperBound="-1"
          eType="#//template/Argument" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="relation" upperBound="-1"
          eType="#//template/Relation" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TemplateVariable" eSuperTypes="platform:/plugin/tools.mdsd.modelingfoundations.identifier/model/identifier.ecore#//Entity">
      <eStructuralFeatures xsi:type="ecore:EReference" name="signature" lowerBound="1"
          upperBound="-1" eType="#//template/Argument"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isHidden" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="valueSpaceDummy" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TemplateFactor" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EReference" name="scope" lowerBound="1"
          upperBound="-1" eType="#//template/TemplateVariable"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Argument">
      <eStructuralFeatures xsi:type="ecore:EReference" name="classProfile" lowerBound="1"
          eType="ecore:EClass ../../tools.mdsd.characteristics/model/characteristics.ecore#//characteristic/CharacteristicProfile"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Relation" abstract="true"/>
    <eClassifiers xsi:type="ecore:EEnum" name="DependenceType">
      <eLiterals name="DIRECTED"/>
      <eLiterals name="UNDIRECTED" value="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ProbabilisticTemplateFactor" eSuperTypes="#//template/TemplateFactor">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="distributionClassDummy"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DependenceRelation" eSuperTypes="#//template/Relation">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1"
          eType="#//template/DependenceType"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
          eType="#//template/TemplateVariable"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
          eType="#//template/TemplateVariable"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TemporalRelation" abstract="true"
        eSuperTypes="#//template/Relation"/>
    <eClassifiers xsi:type="ecore:EClass" name="PresistenceRelation" eSuperTypes="#//template/TemporalRelation">
      <eStructuralFeatures xsi:type="ecore:EReference" name="interfaceVariable" lowerBound="1"
          eType="#//template/TemplateVariable"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TimeSliceRelation" eSuperTypes="#//template/TemporalRelation">
      <eStructuralFeatures xsi:type="ecore:EReference" name="temporalDependence" lowerBound="1"
          eType="#//template/DependenceRelation"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="model" nsURI="http://palladiosimulator.org/Environment/Model/1.0"
      nsPrefix="model">
    <eClassifiers xsi:type="ecore:EClass" name="GroundRandomVariable">
      <eStructuralFeatures xsi:type="ecore:EReference" name="instantiatedTemplate"
          lowerBound="1" eType="#//template/TemplateVariable"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="dependenceStructure"
          lowerBound="1" upperBound="-1" eType="#//template/DependenceRelation"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="classManifestation" lowerBound="1"
          eType="ecore:EClass ../../tools.mdsd.characteristics/model/characteristics.ecore#//manifestation/ProbabilisticManifestation"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="descriptiveModel" lowerBound="1"
          eType="#//model/LocalProbabilisticModel"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LocalProbabilisticModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="instantiatedFactor" lowerBound="1"
          eType="#//template/ProbabilisticTemplateFactor"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="distParamsDummy" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ProbabilisticModelRepository" eSuperTypes="platform:/plugin/tools.mdsd.modelingfoundations.identifier/model/identifier.ecore#//Entity">
      <eStructuralFeatures xsi:type="ecore:EReference" name="models" upperBound="-1"
          eType="#//model/ProbabilisticModel" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ProbabilisticModel" eSuperTypes="platform:/plugin/tools.mdsd.modelingfoundations.identifier/model/identifier.ecore#//Entity">
      <eStructuralFeatures xsi:type="ecore:EReference" name="groundRandomVariables"
          lowerBound="1" upperBound="-1" eType="#//model/GroundRandomVariable" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="localModels" lowerBound="1"
          upperBound="-1" eType="#//model/LocalProbabilisticModel" containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="dynamics" nsURI="http://palladiosimulator.org/Environment/Dynamics/1.0"
      nsPrefix="dynamics">
    <eClassifiers xsi:type="ecore:EClass" name="DynamicBehaviourRepository" eSuperTypes="platform:/plugin/tools.mdsd.modelingfoundations.identifier/model/identifier.ecore#//Entity">
      <eStructuralFeatures xsi:type="ecore:EReference" name="extensions" upperBound="-1"
          eType="#//dynamics/DynamicBehaviourExtension" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DynamicBehaviourExtension">
      <eStructuralFeatures xsi:type="ecore:EReference" name="model" lowerBound="1"
          eType="#//model/ProbabilisticModel"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="behaviour" lowerBound="1"
          eType="#//dynamics/DynamicBehaviour" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DynamicBehaviour" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EReference" name="localModels" lowerBound="1"
          upperBound="-1" eType="#//dynamics/LocalDynamicModel" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="InductiveDynamicBehaviour" eSuperTypes="#//dynamics/DynamicBehaviour">
      <eStructuralFeatures xsi:type="ecore:EReference" name="interTimeSliceInductions"
          lowerBound="1" upperBound="-1" eType="#//dynamics/InterTimeSliceInduction"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="intraTimeSliceEffects"
          upperBound="-1" eType="#//dynamics/IntraTimeSliceEffect" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="InterTimeSliceInduction">
      <eStructuralFeatures xsi:type="ecore:EReference" name="temporalStructure" lowerBound="1"
          upperBound="-1" eType="#//template/TemporalRelation"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="descriptiveModel" lowerBound="1"
          eType="#//dynamics/LocalDynamicModel"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="IntraTimeSliceEffect">
      <eStructuralFeatures xsi:type="ecore:EReference" name="dependenceStructure"
          lowerBound="1" upperBound="-1" eType="#//template/DependenceRelation"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DeductiveDynamicBehaviour" eSuperTypes="#//dynamics/DynamicBehaviour">
      <eStructuralFeatures xsi:type="ecore:EReference" name="stateTransitionMatrix"
          lowerBound="1" eType="#//dynamics/TransitionMatrix" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="observationMatrix" eType="#//dynamics/TransitionMatrix"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LocalDynamicModel">
      <eStructuralFeatures xsi:type="ecore:EReference" name="instantiatedTemplate"
          lowerBound="1" eType="#//template/TemplateFactor"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="distParamsDummy" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TransitionMatrix">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1"
          eType="#//dynamics/TransitionMatrixType"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="rowDistributions" lowerBound="1"
          upperBound="-1" eType="#//dynamics/LocalDynamicModel"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="TransitionMatrixType">
      <eLiterals name="STATE"/>
      <eLiterals name="OBSERVATION" value="1"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
